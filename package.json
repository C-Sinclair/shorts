{
  "name": "shorts",
  "private": true,
  "scripts": {
    "dev": "TAILWIND_MODE=watch astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "db:dev": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "test": "vitest",
    "_test": "start-server-and-test start http://localhost:3000 test:e2e",
    "test:e2e": "npm-run-all -p test:e2e:*",
    "test:e2e:js": "playwright test",
    "test:e2e:no-js": "cross-env DISABLE_JAVASCRIPT=true playwright test"
  },
  "prisma": {
    "enabled": true,
    "schema": "src/db/schema.prisma"
  },
  "devDependencies": {
    "@astrojs/netlify": "^0.2.3",
    "@astrojs/partytown": "^0.1.2",
    "@astrojs/solid-js": "^0.1.1",
    "@astrojs/tailwind": "^0.1.2",
    "@playwright/test": "1.20.0",
    "@prisma/client": "^3.9.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "astro": "^1.0.0-beta.8",
    "autoprefixer": "^10.4.4",
    "npm-run-all": "latest",
    "playwright": "1.19.2",
    "postcss": "^8.4.12",
    "prisma": "^3.9.0",
    "solid-js": "^1.3.0-rc.4",
    "start-server-and-test": "latest",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.4.3",
    "vite": "^2.8.6",
    "vitest": "^0.2.7"
  },
  "dependencies": {
    "@authorizerdev/authorizer-js": "^0.10.0",
    "@felte/solid": "^1.1.7",
    "@felte/validator-vest": "^1.0.6",
    "isomorphic-fetch": "^3.0.0",
    "solid-use": "^0.3.3",
    "vest": "^4.3.2"
  },
  "engines": {
    "node": ">=14"
  }
}
