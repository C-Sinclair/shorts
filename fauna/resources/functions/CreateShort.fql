CreateFunction({
  name: "CreateShort",
  body: Query(Lambda(
    ["title", "description", "url", "tags"],
    Create(
      Collection('Shorts'),
      {
        data: {
          title: Var("title"),
          description: Var("description"),
          url: Var("url"),
          tags: Var("tags"),
        }
      }
    )
  )),
  role: Role("ShortAdminUDF")
})